{
  "manifest_version": 3,
  "name": "Github NewTab",
  "description": "A beautiful new tab page that displays your GitHub contribution graph, stats, and coding activity in an elegant cosmic-themed interface.",
  "version": "1.5.1",
  "action": {
    "default_icon": {
      "16": "icon-32.png",
      "32": "icon-32.png",
      "48": "icon-128.png",
      "128": "icon-128.png"
    },
    "default_title": "Github States"
  },
  "chrome_url_overrides": {
    "newtab": "src/pages/newtab/index.html"
  },
  "icons": {
    "16": "icon-32.png",
    "32": "icon-32.png",
    "48": "icon-128.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "activeTab"
  ],
  "host_permissions": [
    "https://api.github.com/*",
    "https://sp-card-t.vercel.app/*",
    "https://github-readme-states-repo-self-inst.vercel.app/*",
    "https://cdn.jsdelivr.net/*",
    "https://github-readme-streak-stats-chi-three.vercel.app/*"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "contentStyle.css",
        "icon-128.png",
        "icon-32.png"
      ],
      "matches": []
    }
  ]
}